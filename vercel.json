{
    "version": 2,
    "builds": [
        {
            "src":"./index.py",
            "use":"@vercel/python",
            "config": {"runtime": "python3.12"}
        },
        { 
            "src": "*.html", 
            "use": "@vercel/static" 
        },
        { 
            "src": "*.js", 
            "use": "@vercel/node"
        }

    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"/"
        }
    ]
}